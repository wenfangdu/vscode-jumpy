{
    "name": "vscode-jumpy",
    "displayName": "jumpy",
    "description": "Jumpy provides fast cursor movement, inspired by Atom's package of the same name.",
    "icon": "images/jumpy-icon.png",
    "license": "MIT",
    "version": "0.3.3",
    "publisher": "krnik",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#1e1e1e",
        "theme": "dark"
    },
    "keywords": [
        "jumpy",
        "jump",
        "AceJump",
        "EasyMotion"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/krnik/vscode-jumpy.git"
    },
    "bugs": {
        "url": "https://github.com/krnik/vscode-jumpy/issues"
    },
    "homepage": "https://github.com/krnik/vscode-jumpy/blob/master/README.md",
    "activationEvents": [
        "onCommand:extension.jumpy-word",
        "onCommand:extension.jumpy-line",
        "onCommand:extension.jumpy-exit"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [],
        "keybindings": [
            {
                "key": "shift+enter",
                "command": "extension.jumpy-exit",
                "when": "editorTextFocus && jumpy.isJumpyMode"
            },
            {
                "key": "shift+enter",
                "command": "extension.jumpy-word",
                "when": "editorTextFocus && !jumpy.isJumpyMode"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Jumpy Configuration",
            "properties": {
                "jumpy.wordRegexp": {
                    "type": "string",
                    "default": "",
                    "description": "The Regexp to use to match words in Jumpy Word Mode"
                },
                "jumpy.lineRegexp": {
                    "type": "string",
                    "default": "",
                    "description": "The Regexp to use to match empty lines in Jumpy Line Mode"
                },
                "jumpy.fontFamily": {
                    "type": "string",
                    "default": "",
                    "description": "Font used in Jumpy decorations, defaults to font from settings"
                },
                "jumpy.fontSize": {
                    "type": "number",
                    "default": 0,
                    "description": "Font size used in Jumpy decorations, defaults to font size from settings"
                },
                "jumpy.darkThemeBackground": {
                    "type": "string",
                    "default": "#004455",
                    "description": "Background of Jumpy decoration in dark themes"
                },
                "jumpy.darkThemeForeground": {
                    "type": "string",
                    "default": "#0af0c1",
                    "description": "Text color of Jumpy decoration in dark themes"
                },
                "jumpy.lightThemeBackground": {
                    "type": "string",
                    "default": "",
                    "description": "Background of Jumpy decoration in light themes"
                },
                "jumpy.lightThemeForeground": {
                    "type": "string",
                    "default": "",
                    "description": "Text color of Jumpy decoration in light themes"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc ",
        "watch": "tsc --watch",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "format": "eslint --fix ./src/**/*"
    },
    "devDependencies": {
        "@types/mocha": "5.2.7",
        "@types/node": "12.7.4",
        "@types/vscode": "1.14.0",
        "@typescript-eslint/eslint-plugin": "2.2.0",
        "@typescript-eslint/parser": "2.2.0",
        "eslint": "6.3.0",
        "eslint-config-prettier": "6.3.0",
        "eslint-plugin-prettier": "3.1.0",
        "mocha": "6.2.0",
        "prettier": "1.18.2",
        "typescript": "3.6.2",
        "vscode-test": "1.2.0"
    }
}
